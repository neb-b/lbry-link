{
  "name": "LBRY Link",
  "version": "0.0.4",
  "description": "Finds LBRY URLs on a page and adds a link tag that opens the LBRY desktop app",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.github.com/*",
        "*://*.twitter.com/*",
        "*://*.youtube.com/*",
        "*://*.reddit.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
